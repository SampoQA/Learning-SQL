CREATE TABLE User
(
UsertID INTEGER PRIMARY KEY,  -- уникальный номер айди
First_name VARCHAR(255) NOT NULL, -- имя строка, длина 255 не может быть пустым
Last_name VARCHAR(255) NOT NULL,  -- фамилия строка, длина 255 не может быть пустым
Country VARCHAR (255) NOT NULL, -- страна строка, длина 255 не может быть пустым
Language VARCHAR (255) NULL,-- язык строка, длина 255 пустое 
Date_birth DATETIME NULL -- Дата рождения формат дата, длина 255 пустое
);

